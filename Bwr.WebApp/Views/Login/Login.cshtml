            <div class="form-group">
                <label class="control-label visible-ie8 visible-ie9">كلمة المرور</label>
                <input class="form-control form-control-solid placeholder-no-fix" type="password" autocomplete="off" placeholder="كملة المرور" name="password" required />
            </div>
            <div class="form-actions">
                <div class="row">
                    <div class="col-lg-4">
                        <button type="button" class="btn green uppercase" onclick="login()">تسجيل الدخول</button>

